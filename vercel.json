{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(health|test)",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    }
  ]
}